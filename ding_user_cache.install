<?php
/**
 * @file
 * Handels schema definition for the module.
 */

/**
 * Implements hook_schema().
 *
 * Defines a cache table for the module.
 */
function ding_user_cache_schema() {
  $schema = array();
  $schema['cache_ding_user_cache'] = drupal_get_schema_unprocessed('system', 'cache');
  return $schema;
}